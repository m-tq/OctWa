{
  "name": "@octwa/sdk",
  "version": "1.1.1",
  "description": "Official TypeScript SDK for integrating dApps with OctWa Wallet Extension. Implements capability-based authorization with full HFHE support.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "octwa",
    "octra",
    "wallet",
    "sdk",
    "blockchain",
    "dapp",
    "web3",
    "hfhe",
    "homomorphic-encryption",
    "capability-based-auth",
    "typescript"
  ],
  "author": "Octra Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-tq/OctWa.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/m-tq/OctWa/tree/master/packages/sdk",
  "bugs": {
    "url": "https://github.com/m-tq/OctWa/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "fast-check": "^3.15.0",
    "jsdom": "^27.3.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
